{
  "name": "@nodots-llc/gnubg-hints",
  "version": "1.0.2",
  "description": "GNU Backgammon hint engine for Node.js with TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gnubg-hints-cli": "dist/cli.js"
  },
  "scripts": {
    "configure": "node ./bin/run-node-gyp.js configure",
    "build:native": "node ./bin/run-node-gyp.js build",
    "build:ts": "tsc",
    "build": "node ./bin/run-node-gyp.js build && tsc",
    "rebuild": "node ./bin/run-node-gyp.js rebuild && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage --verbose",
    "install": "node ./bin/run-node-gyp.js rebuild",
    "publish:npm": "npm publish --access public",
    "prepublishOnly": "node ./bin/run-node-gyp.js build && tsc",
    "pretest": "node ./bin/run-node-gyp.js build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@nodots-llc/backgammon-types": "^4.1.1",
    "node-addon-api": "^7.0.0",
    "node-gyp": "^10.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "gypfile": true,
  "keywords": [
    "backgammon",
    "gnubg",
    "hints",
    "ai",
    "native",
    "addon",
    "typescript"
  ],
  "author": "",
  "license": "GPL-3.0"
}
